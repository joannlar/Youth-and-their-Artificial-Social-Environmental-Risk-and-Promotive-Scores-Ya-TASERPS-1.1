breed [youths youth]
breed [probs prob]

globals [
  home-patches         ;; agentset of blue patches representing the home areas
  school-patches       ;; agentset of brown patches representing the school areas
  neighborhood-patches ;; agentset of green patches representing the neighborhood areas
  to-prosocial
  to-antisocial
]

youths-own [
  individual-risk           ;; an individual's probability of engaging in antisocial behavior
  individual-pro            ;; an individual's probability of engaging in a prosocial behavior
  prosocial                 ;; an individual's tally of total prosocial experiences
  antisocial                ;; an individual's tally of total antisocial experiences

  family-risk               ;; an individual family's risk probability
  family-pro                ;; an individual family's promotive probability

  caught                    ;; tracks the number of times a youth is arrested
  risk-cat                  ;; an individual's risk category according to a standardized risk assessmentt
  on-prob                   ;; tracks how long a youth is on probation
  visits                    ;; number of times youth has visited a probation officer while on probation
]

probs-own [
  pun-tx                    ;; where on the punitive-treatment continuum the p.o. falls (1 (pun)-5 (tx))

]

patches-own [
  pro-opp      ;; each patch has a prosocial opportunity score
  risk-opp     ;; each patch has a risk opportunity score

  hot-spot     ;; tracks the occurrence of antisocial events
]


to setup
  clear-all
  resize-world -16 16 -20 20

  ;; create the home patches
  set home-patches patches with [pycor < -15]
  ask home-patches [
    set pcolor blue
    set pro-opp 1       ;; family risk sits with the youth, as their "home" and "family" aren't location specific
    set risk-opp 1    ;; family risk sits with the youth, as their "home" and "family" aren't location specific
  ]

  ;; create the school patches
  set school-patches patches with [pycor > 15]
  ask school-patches [
    set pcolor brown

    ;; Risk and protective opportunities that randomly fall along the exponential distribution
    ;; centered on user-controlled mean,
    ;; but bounded between .01 and .99 so this variable can operate as a probability

    set risk-opp random-exponential-in-bounds  (schools-risk / 100) .01 .99
    set pro-opp random-exponential-in-bounds (schools-promotive / 100) .01 .99
    set hot-spot 0
  ]

  ;; create the neighborhood patches
  set neighborhood-patches patches with [ (pycor < 16) and (pycor > -16)]
  ask neighborhood-patches [
    set pcolor green

    ;; Risk and protective opportunities that randomly fall along the exponential distribution
    ;; centered on user-controlled mean,
    ;; but bounded between .01 and .99 so this variable can operate as a probability

    set risk-opp random-exponential-in-bounds (neighborhood-risk / 100) .01 .99
    set pro-opp random-exponential-in-bounds (neighborhood-promotive / 100) .01 .99

    ;; The is a "hot spot" tracker which counts the number of times an antisocial activity
    ;; occurs at that location.
    set hot-spot 0
  ]

  ;; These variables track the number of times any youth switches into the prosocial and
  ;; antisocial catetgory after being categorized in the other group.
  set to-prosocial 0
  set to-antisocial 0

  ;; create the youths
  let NUM-YOUTH 200
  create-youths NUM-YOUTH [
    set color violet
    set size 1.0
    set shape "person"
    set heading 0

    ;; The family and risk protective factors randomly fall along the exponential distribution
    ;; centered on user-controlled mean,
    ;; but bounded between .01 and .99 so this variable can operate as a probability.

    set family-risk random-exponential-in-bounds (risk-level / 100) .001 .999
    set family-pro random-exponential-in-bounds (promotive-level / 100) .001 .999

    ;; Individual risk and protective factor is based on the family's risk and protective
    ;; factor, since there will be varying influence of family on youth.
    ;; Youth score is centered on family's score and is drawn randomly from the
    ;; normal distribution, but bounded between .01 and .99 so this variable
    ;; can perate as a probability. Standard deviation is arbitrarily set at .02.

    set individual-risk random-normal-in-bounds family-risk .02 .001 .999
    set individual-pro random-normal-in-bounds family-pro .02 .001 .999

    ;; the model starts with the youth as a blank slate, so 0 values for prosocial and antisocial.

    set prosocial 0
    set antisocial 0

    ;; these aree varaibles relate to the youth's justice involvement: the number of times
    ;; they are arrested, their risk category as would be measured based on a standardized
    ;; risk assessment, whether they are "on probation" and the number of times they've
    ;; visited an authority figure while on probation.
    set caught 0
    set risk-cat 0
    set on-prob FALSE
    set visits 0

    ;; start all the youth at home
    move-to one-of home-patches
  ]

  ;; create the authority figures
  if prob-status != "none" [
     create-probs NUM-PROBS[
       set color blue
       set size 1.5
       set shape "person"
       set heading 0

      ;; set the probation officer's personal philosophy
      ;; along the punishment versus rehabilitation continum.

      set pun-tx random 5 + 1

      ; place the authority figure in the neighborhood.

      move-to one-of neighborhood-patches

     ]
  ]
  reset-ticks

end

to go


  ;; Each tick represents a day. This is the only way for the model to stop,
  ;; depending on the number of days you want this to run.

  let MAX-TICKS 1800
  repeat MAX-TICKS [
    step
  ]

end

;; To move through a day, youth go to school, there's an after school period,
;; then youth are at home in the evenings. Then the ticks steps forward.

to step
  youths-go-to-school
  youths-after-school
  youths-evenings
  tick
end



to youths-go-to-school
  ask youths [move-to one-of school-patches]

  ;; Set school day to 6 hours, (e.g., 7am-2pm).
  ;; At each hour, if the youth is in school, the youth will make a decision about whether to leave early.
  ;; If they've already left school, that youth will follow the procedure for being in the community.

  let SCHOOL-HOUR 6
  repeat SCHOOL-HOUR [
    ask youths [
       if (member? patch-here school-patches) [youth-leaves-early]
       if (member? patch-here neighborhood-patches) [youth-in-neighborhood]
    ]
  ]

  ;; At the end of the school day, if the youth is still in school (i.e., stayed the whole day):
  ;; depending on the schools protective score (a probability which represents
  ;; the likelihood of a teacher, class, etc being positively impactful on the youth)
  ;; the youth's prosocial tally will go up. Then, the youth moves into the neighborhood.

  ;; If not still in school, depending on the neighborhood risk score (a probability which
  ;; represents the likelihood that they will meet a deviant peer, etc), the youth's
  ;; antisocial tally will go up.

  ask youths [
    ifelse (member? patch-here school-patches) [
      if(random-float 1.00 < pro-opp) [set prosocial prosocial + .10]
      move-to one-of neighborhood-patches]
    [ if random-float 1.00 < risk-opp [set antisocial antisocial + .10 ]]
  ]
end



to youths-after-school

  ;; Set after school time to 5 hours (e.g., 2-7pm).
  ;; Here, youth are allowed to roam around the neighborhood.
  ;; For youth on probation, the authority figure decides whether to close
  ;; the youth's case.

  probs-move
  let AFTER-HOUR 5
  repeat AFTER-HOUR [
    ask youths [
      youth-in-neighborhood
     ]
      probs-close-case
  ]
end


to probs-move

  ;; the authority figures may choose to move around the neighborhood.
  ;; they will move toward locations where there are more antisocial activities.

  ask probs [
    if random-float 1.00 < .20 [set heading random 360]
       move-to max-one-of neighbors [hot-spot]
  ]
end

to probs-close-case

  ;; to determine if an authority figure will close a case, they first
  ;; identify whether any of the youth around thetm are on probation.
  ;; All the adjacent youth are considered to have "visited" the
  ;; probation office, so the number of visits is incremented by 1.

  ask probs[
    if any? youths-on one-of neighbors [
      ask youths-on neighbors [
        if on-prob = true [set visits visits + 1]

        ;; Next, the authority figure decides whether to close the youth's case.
        ;; If the youth has been categorized as low-risk, the case will be closed
        ;; once the youth has visited the office 8 times. Their individual risk score
        ;; will decrease and individual promotive score will increase.
        ;; For youth who are medium or high risk, there is a probability their
        ;; case will be closed once they reach a minimum threshold of visits.
        ;; Additionally, there is a probability their risk and promottive scores
        ;; will be reduced and increased, respectively.

        if (visits > 8 and risk-cat = 1) [
          close-case
          set risk-cat risk-cat + 1
          if prob-status = "recidivism" [set individual-risk individual-risk - .01]
          if prob-status = "prosocial" [set individual-risk individual-risk - .01
            set individual-pro individual-pro + .01]
          ]
        if visits > 15 and risk-cat = 2 [
          if random-float 1.00 < .75 [
            close-case
            set risk-cat risk-cat + 1
            if random-float 1.00 < .66 [
              if prob-status = "recidivism" [set individual-risk individual-risk - .01]
            if prob-status = "prosocial" [set individual-risk individual-risk - .01
              set individual-pro individual-pro + .01]]
          ]
        ]
        if visits > 22 and risk-cat = 3 [
          if random-float 1.00 < .50 [
            close-case
            if random-float 1.00 < .20 [
              if prob-status = "recidivism" [set individual-risk individual-risk - .01]
            if prob-status = "prosocial" [set individual-risk individual-risk - .01
                set individual-pro individual-pro + .01]]
           ]
         ]
    ]
  ]
  ]

end


to close-case

  ; this is the equivalent of administrative paperwork.
  ; reset the youth's variables so they are not on probation.
  set on-prob false
  set visits 0
  set-youth-color
end

to youths-evenings

  ;; Set the evening duration with family to 4 hours (e.g., 7-11pm).

  let EVENING-HOUR 4
  repeat EVENING-HOUR [
    ask youths [
      ifelse (member? patch-here home-patches) [youth-at-home]
      [move-to one-of home-patches]
    ]
  ]

  ;; This provides an indication of whether the youth has had more
  ;; antisocial experiences (red) or prosocial experiences (lime).

  ask youths [set-youth-color]
end

to set-youth-color

  ;; for the simulation, we can observe youth who are categorized as
  ;; prosocial as green,
  ;; antisocial as red;
  ;; on probation as purple.

  ifelse on-prob = false [
    ifelse antisocial > prosocial [
      if color = lime [set to-antisocial to-antisocial + 1]
      set color red ]
      [
      if color = red [set to-prosocial to-prosocial + 1]
      set color lime ]
  ] [set color violet]
end


to youth-leaves-early

  ;; If the youth is in school, chance of leaving early depends on individual and school risk.
  ;; Depending on the location's prosocial opportunity probability (pro-opp), the youth may stay in school
  ;; (i.e., move to a different location within school).

  ;; If not, depending on the location's risk opportunity probability (risk-opp),
  ;; the youth may be faced with an antisocial opportunity. Depending on the
  ;; youth's risk score (probability), they may choose to leave school early.

  ifelse (member? patch-here school-patches) and (random-float 1.00 < pro-opp )
      [move-to one-of school-patches]
  [if (member? patch-here school-patches) and (random-float 1.00 < risk-opp)
    [if random-float 1.00 < individual-risk [move-to one-of neighborhood-patches]]
    ]
end

to youth-in-neighborhood

  ;; First, have youth move randomly, but
  ;; make sure they stay in the neighborhood patches.

    if random-float 1.00 < .10 [set heading random 360]
    if (patch-ahead 1 != nobody and patch-ahead 2 != nobody) [
       if member? patch-ahead 1 neighborhood-patches [
          if not any? probs-on patch-ahead 1 and not any? probs-on patch-ahead 2 [
            forward 1]
       ]
  ]

    ;; While the youth is in the community, based on the youth's location,
    ;; there will be a chance (pro-opp) the youth
    ;; encounters a positive influence and/or has a pro-social opportunity.

    ;; If the youth does not have a prosocial opportunity, based on the youth's location,
    ;; there will be a chance (risk-opp) the youth
    ;; encounters a negative influence or has an anti-social opportunity.

    ifelse random-float 1.00 < ( pro-opp) [
      youth-meets-positive-influence]
   [if random-float 1.00 < ( risk-opp) [youth-meets-negative-influence] ]
end


;; If the youth meets a positive influence, there is a chance based on the youth's
;; protective factor (individual-pro) that they will seize the opportunity. If they
;; choose the positive opportunity, their prosocial experience score goes up.

to youth-meets-positive-influence
    if random-float 1.00 < (individual-pro ) [
      set prosocial prosocial + .10
      if prob-status = "prosocial" [
        if on-prob [
        if individual-pro < .999 [set individual-pro individual-pro + .001]
      ]

    ]
  ]
end

;; If the youth meets a negative influence, there is a chance based on the youth's
;; risk factor (individual-risk) that they will seize the opportunity.

to youth-meets-negative-influence
  if not any? probs-on patch-here[
    ifelse prob-status = "deterrent" [
      if not any? probs-on neighbors [
        youth-takes-negative-opportunity]]
      [youth-takes-negative-opportunity]
    ]
end


;; if the youth takes the negattive opportunity,their antisocial experiences will
;; go up. We consider whether there is an authority figure nearby. The location
;; increments the number of antisocial activities (hot-spot).

to youth-takes-negative-opportunity
   if random-float 1.00 < individual-risk [
      set antisocial antisocial + .10
      probation-officer-nearby
      ask patch-here [
         set hot-spot hot-spot + 1]
  ]
end

to probation-officer-nearby

  ;; create a local variable to aid in the multiple-step decision-making process.
  let arrested false

  ;; if there are any authority figures on the neighborhoing locations:
  ;; if they have been categorized as antisocial, indicate that they
  ;; are "arrested". Otherwise, depending on how punitive the
  ;; authority figure is, there is a probability that the authority
  ;; figure will arrest the youth and put them on probation.
  if any? probs-on neighbors[
    ifelse (antisocial > prosocial) [set arrested true]
    [ask one-of probs-on neighbors[
      if pun-tx = 1 [if random-float 1.00 < .75 [set arrested true]]
      if pun-tx = 2 [if random-float 1.00 < .65 [set arrested true]]
      if pun-tx = 3 [if random-float 1.00 < .50 [set arrested true]]
      if pun-tx = 4 [if random-float 1.00 < .25 [set arrested true]]
      if pun-tx = 5 [if random-float 1.00 < .10 [set arrested true]]
    ]
    ]

    ;; if the youth is arrested, set the youth's risk category,
    ;; which simulates the administration of a risk assessment
    ;; at intake.
    set-risk-cat

    ;; now, revisit the "arrest" decision.
    ;; depending on the youth's risk category, change the "arrested" status.
    ;; Overall, low risk youth are less likely to be "arrested"
    ;; and high risk youth are mostt likely to be "arrested"

    if risk-cat < 2 [if random-float 1.00 < .75 [set arrested false]]
    if risk-cat = 2 [if random-float 1.00 < .50 [set arrested false]]


    ;; now the decision is complete. if the youth is "arresttetd"
    ;; indicate that the youth is on probation,
    ;; increment their legal history (caught)
    ;; and decide their sanction.

    if arrested = true [
      set caught caught + 1
      set on-prob TRUE
      set color violet
      set-youth-sanction]
    ]
end

;; update the individual risk score -- but make sure it never exceeds 1.0
;; since it is treated as a probability.

to update-risk
  if individual-risk < .999 [set individual-risk individual-risk + .001]
end

;; establish the youth's risk category based on their individual risk score.
;; this is probabilistic, since a risk assessment will be imperfect and there
;; may be mistakes in completing the risk assessment.
to set-risk-cat
  if risk-cat = 0 [
    if prob-status = "recidivism" [
      ifelse individual-risk > .75 [set risk-cat 3]
        [ifelse individual-risk > .5 [set risk-cat 2] [set risk-cat 1]]
    ]
    if prob-status = "prosocial" [
      let buffer individual-risk - individual-pro
      ifelse buffer > .75 [set risk-cat 3]
        [ifelse buffer > .5 [set risk-cat 2] [set risk-cat 1]]
    ]
  ]


  ;; if the youth has been on probation before, update the risk category.
  ;; the youth's risk score is re-evaluated based on both their individual
  ;; risk score, and additionally will increase due to their arrest history.

  if risk-cat = 1 [
    if prob-status = "recidivism" [
      if individual-risk > .5 and caught > 3 [set risk-cat 2]
    ]
    if prob-status = "prosocial" [
      let buffer individual-risk - individual-pro
      if buffer > .5 and caught > 3 [set risk-cat 2]
    ]
  ]
  if risk-cat = 2 [
    if prob-status = "recidivism" [
      if individual-risk > .75 and caught > 20 [set risk-cat 3]
    ]
    if prob-status = "prosocial" [
      let buffer individual-risk - individual-pro
      if buffer > .75 and caught > 20 [set risk-cat 3]
  ]
  ]
end

;; determine the youth's sanction. Only some youth
;; who are arrested will be put on probation.

to set-youth-sanction
  if risk-cat > 1 [set on-prob true]
  if risk-cat = 1 and caught > 5 [set on-prob true]
end


to youth-at-home

  ;; While at home, youth might take in family risk or protective influence.
  ;; Based on the family risk score (probability), and ensuring the individual-risk score stays within the
  ;; bounds of a probability, additionally, whether the youth's risk score is higher than protective score,
  ;; the youth's individual risk score(probability) may go up, or if not, the youth's individual risk score
  ;; may go down.

  if remainder ticks 180 = 0 [

  ifelse (random-float 1.00 < (family-risk )) and (individual-risk < .980 ) and (antisocial > prosocial) [
    set individual-risk individual-risk + .001
  ] [ if individual-risk > .02 and (antisocial < prosocial) [set individual-risk individual-risk - .001 ]]


  ;; Based on the family protective score (probability), and ensuring the individual-protective score stays within the
  ;; bounds of a probability, additionally, whether the youth's protective score is higher than risk score,
  ;; the youth's individual protective score(probability) may go up, or if not, the youth's individual protective score
  ;; may go down.

  ifelse (random-float 1.00 < (family-pro )) and (individual-pro < .980) and (prosocial > antisocial) [
       set individual-pro individual-pro + .001
  ] [ if individual-pro > .02 and (prosocial < antisocial) [set individual-pro individual-pro - .001 ] ]
  ]
end


;; This procedure checks to see that the value produced by the random-normal
;; function stays within bounds. If not, another number is randomly
;; drawn.

to-report random-normal-in-bounds [med dev mmin mmax]
  let result random-normal med dev
  if result < mmin or result > mmax
    [ report random-normal-in-bounds med dev mmin mmax ]
  report result
end

;; This procedure checks to see that the value produced by the random-exponential
;; function stays within bounds. If not, another number is randomly
;; drawn.

to-report random-exponential-in-bounds [med mmin mmax]
  let result random-exponential med
  if result < mmin or result > mmax
    [ report random-exponential-in-bounds med mmin mmax ]
  report result
end
